<script type="text/javascript">

var navToggleButton = document.getElementsByClassName('nav-toggle');

for (i=0; i<navToggleButton.length; i++) {
	var navToToggle = navToggleButton[i].nextElementSibling;

	navToggleButton[i].addEventListener("click", function(e) {
		toggleClass(navToToggle, 'nav-show');
	})
}

// nextSibling can include text nodes
function nextElementSibling(el) {
  do { el = el.nextSibling; } while ( el && el.nodeType !== 1 );
  return el;
}
// el.nextElementSibling || nextElementSibling(el);

function toggleClass(el, className) {
	if (el.classList) {
	  el.classList.toggle(className);
	} else {
	    var classes = el.className.split(' ');
	    var existingIndex = -1;
	    for (var i = classes.length; i--;) {
	      if (classes[i] === className)
	        existingIndex = i;
	    }

	    if (existingIndex >= 0)
	      classes.splice(existingIndex, 1);
	    else
	      classes.push(className);

	  el.className = classes.join(' ');
	}
}

</script>